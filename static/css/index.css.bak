* {
	font-size: 10pt;
	font-family: "Helvetica Neue", "Luxi Sans", "DejaVu Sans", "Hiragino Sans GB", "Microsoft YaHei" , "微软雅黑";
	border: 0;
	margin: 0;
	padding: 0;
}
html
{
  height: 100%;
}
body {
	margin: 0;
	padding: 0;
	height: 100%;
	overflow: auto;
	background: #f6f7f7;
}
/*滚动条样式*/
::-webkit-scrollbar
{
    width: 6px;
    height:6px;
    background-color: #eaeaea;
}
::-webkit-scrollbar-track
{
    border-radius: 10px;
    background-color: #f7f8f6;
}
::-webkit-scrollbar-thumb
{
    border-radius: 10px;
    background-color: #eaeaea;
}
div {
	box-sizing: border-box;
}
.main-box{margin-top: 75px;}
.body{padding-left: 200px;padding-top: 50px;}
 .index .ivu-btn{padding: 6px 20px;margin-left:10px}
.ivu-btn-primary{background: #6c91f0;border: 1px solid #6c91f0;}
.ivu-btn-danger{background: #ed3f14cf !important;border: 1px solid #ed3f14cf !important;color: #FFF !important;}
.ivu-btn-success{background: #37cd8f;border: 1px solid #37cd8f;}
.ivu-page{display: inline-block;width: 100%;height: 55px;line-height: 55px;margin: 10px 0;text-align: right;}
.ivu-table th{text-align: center;}
.ivu-table .ivu-table-cell{text-align: center;}
.ivu-page-item-active{    background-color: #8791a5; border-color: #8791a5;}
.ivu-table-tbody .ivu-btn>.ivu-icon{font-size: 20px;}
.ivu-table-tbody .ivu-btn>.ivu-icon:hover{color: #c3c3c3;}
.ivu-table-wrapper{border: 0px;}
.ivu-table-wrapper .ivu-table .ivu-table-body table{width:100% !important}
.ivu-table:after{display: none;}
.ivu-table td, .ivu-table th{border-bottom: 1px  #ecedf3;}
/* solid */
.ivu-table th{background: none;color: #c0c0c0;}
.ivu-table-border td, .ivu-table-border th{border-right: 0px solid #e9eaec;}
.ivu-input-group-append .ivu-btn{padding: 6px 15px;}
.ivu-modal-wrap .ivu-modal-body{max-height: 492px;overflow: auto;}
.ivu-table:before{display: none;}
.ivu-btn-error{background: #FF5240;border: 1px solid #FF5240;}

.ivu-table-column-center .ivu-tooltip-rel .ivu-btn{padding: 0; border: 0; width: 35px;}
.ivu-table-column-center .ivu-tooltip-rel .ivu-btn .ivu-icon{border-radius: 50%;padding: 4px 4px;height: 30px;width: 30px;font-size: 18px;line-height: 23px !important;color: #9f9f9f;}
.ivu-table-column-center .ivu-tooltip-rel .ivu-btn .ivu-icon:hover{color: #FFFFFF;background: rgb(158, 122, 229);}
.ivu-table-column-center .ivu-table-cell .ivu-tooltip{margin: 0px 5px;}

.Path{height: 60px;line-height: 60px;background: #fff;padding: 15px 15px 15px 15px;}
.Path .ivu-select{float: left;}
